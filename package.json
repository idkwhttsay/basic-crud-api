{
  "name": "basic-crud-api",
  "version": "1.0.0",
  "description": "Basic CRUD API backend application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc src/index.ts",
    "test": "jest --silent",
    "test:verbose": "jest",
    "start:prod": "tsc && webpack --mode=production && node dist/src/index.js",
    "start:dev": "nodemon src/index.ts",
    "start:multi": "ts-node src/load.balancer.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "start": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
